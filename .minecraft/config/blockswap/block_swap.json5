{
	// Generates all block states for all blocks in the registry.
	"generate_all_known_states": true,
	// Whether blocks are replaced in existing chunks.
	"retro_gen": false,
	/* A map of states that specifies what the "old" block state is and what its "new" block state is.
	   See "known_states" folder("generate_all_known_states" must be set to true in this config) to see all known block states available for all blocks available in the registry.
	   Example:
	    "state_swapper": [
	    	{
	    		"new": {
	    			"Name": "minecraft:birch_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "x"
	    			}
	    		},
	    		"old": {
	    			"Name": "minecraft:oak_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "z"
	    			}
	    		}
	    	},
	    	{
	    		"new": {
	    			"Name": "minecraft:birch_leaves",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"distance": "7",
	    				"persistent": "true"
	    			}
	    		},
	    		"old": {
	    			"Name": "minecraft:acacia_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "z"
	    			}
	    		}
	    	},
	    	{
	    		"new": {
	    			"Name": "minecraft:jungle_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "x"
	    			}
	    		},
	    		"old": {
	    			"Name": "minecraft:birch_log",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"axis": "z"
	    			}
	    		}
	    	},
	    	{
	    		"new": {
	    			"Name": "minecraft:jungle_planks",
	    		},
	    		"old": {
	    			"Name": "minecraft:acacia_planks",
	    			}
	    		}
	    	}
	    ]
	*/
	"state_swapper": [],
	/* A map of blocks that specifies what the "old" block is and what its "new" block is.
	   Example:
	   	"swapper": {
	   		"minecraft:coarse_dirt": "minecraft:dirt",
	   		"minecraft:diamond_block": "minecraft:emerald_block"
	   	}
	*/
	"swapper": {
		"biomesoplenty:dead_log" : "regions_unexplored:dead_log",
		"biomesoplenty:dead_wood" : "regions_unexplored:dead_wood",
		"biomesoplenty:dead_leaves" : "regions_unexplored:dead_leaves",
		"regions_unexplored:willow_log" : "environmental:willow_log",
		"biomesoplenty:willow_log" : "environmental:willow_log",
		"regions_unexplored:willow_leaves" : "environmental:willow_leaves",
		"biomesoplenty:willow_leaves" : "environmental:willow_leaves",
		"biomesoplenty:palm_leaves" : "regions_unexplored:palm_leaves",
		"biomesoplenty:palm_log" : "regions_unexplored:palm_log",
		"biomesoplenty:palm_wood" : "regions_unexplored:palm_wood",
		"regions_unexplored:barley" : "biomesoplenty:barley",
		"biomesoplenty:cattail" : "environmental:cattail",
		"regions_unexplored:cattail" : "environmental:cattail",
		"regions_unexplored:clover" : "biomesoplenty:clover",
		"regions_unexplored:pine_log" : "environmental:pine_log",
		"regions_unexplored:pine_leaves" : "environmental:pine_leaves",
		"regions_unexplored:dead_pine_leaves" : "environmental:pine_leaves",
		"biomesoplenty:redwood_log" : "regions_unexplored:redwood_log",
		"biomesoplenty:redwood_leaves" : "regions_unexplored:redwood_leaves",
		"biomesoplenty:maple_leaves" : "biomesoplenty:orange_autumn_leaves",
		"environmental:giant_tall_grass" : "biomesoplenty:high_grass"
	}
}